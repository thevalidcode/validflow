{
  "identifier": "com.validdeck.validflow",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "productName": "ValidFlow",
  "version": "1.5.0",
  "app": {
    "windows": [
      {
        "title": "ValidFlow",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/validflow.icns",
      "icons/validflow.ico",
      "icons/validflow.png"
    ],
    "resources": [],
    "externalBin": [
      "bin/start"
    ],
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "minimumSystemVersion": "10.13",
      "signingIdentity": null
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "plugins": {
    "shell": {}
  }
}